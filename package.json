{
  "name": "vite-plugin-mock-pro",
  "version": "0.1.0",
  "type": "module",
  "description": "一个为 Vite 打造的、功能强大且轻量级的 Mock API 插件。支持热模块更新 (HMR)，用 TypeScript 编写，并提供完整的类型定义。",
  "author": {
    "name": "angryoro"
  },
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run --coverage",
    "build": "npm test && tsc --project tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fast-glob": "^3.3.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "connect": "^3.7.0",
    "eventsource": "^2.0.2",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "vite": "^5.2.0",
    "vitest": "^3.2.4"
  }
}
